{
    "version": 2,
    "builds": [
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "/public/$1"
        },
        {
        "src": "/api/save-time",
        "dest": "/api/save-time.js"
        },
        {
        "src": "/api/get-time",
        "dest": "/api/get-time.js"
        }
    ]
  }
  